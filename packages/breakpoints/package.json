{
  "name": "@bedrock-ui/breakpoints",
  "version": "0.1.0",
  "description": "Bedrock UI Breakpoints",
  "files": [
    "lib"
  ],
  "module": "lib/esm/index.js",
  "main": "lib/cjs/index.js",
  "types": "lib/cjs/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc --project tsconfig.build-cjs.json && tsc-alias -p tsconfig.build-cjs.json",
    "build:esm": "tsc --project tsconfig.build-esm.json && tsc-alias -p tsconfig.build-esm.json",
    "dev": "start-storybook -p 6006",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "author": "Matthew Wolfe",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "babel-loader": "^8.2.5",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^28.0.7",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  }
}
